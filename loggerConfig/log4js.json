{
    "appenders": {
      "consoleFiltered": {
        "type": "logLevelFilter",
        "appender": "consoleLog",
        "level": "DEBUG"
      },
      "consoleLog": {
        "type": "stdout",
        "level": "DEBUG"
      },
      "fileLog": {
        "type": "file",
        "filename": "logs/log.log",
        "pattern": "-yyyy-MM-dd"
      }
    },
    "categories": {
      "default": { "appenders": [ "consoleLog", "fileLog" ], "level": "TRACE"}
    }
  }